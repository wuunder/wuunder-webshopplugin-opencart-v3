{{ header }}{{ column_left}}
    <div id="content">
      <div class="page-header">
          <div class="container-fluid">
              <div class="pull-right">
                  <button type="submit" form="form-category" data-toggle="tooltip" title="{{ button_save }}"
                          class="btn btn-primary"><i class="fa fa-save"></i></button>
                  <a href="{{ cancel }}" data-toggle="tooltip" title="{{ button_cancel }}"
                     class="btn btn-default"><i class="fa fa-reply"></i></a></div>
              <h1>{{ heading_title }}</h1>
              <ul class="breadcrumb">
                  {% for breadcrumb in breadcrumbs %}
                    <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
                  {% endfor %}
              </ul>
          </div>
      </div>
        <div class="container-fluid">
            {% if error_warning %}
                <div class="alert alert-danger"><i class="fa fa-exclamation-circle"></i> {{ error_warning }}
                    <button type="button" class="close" data-dismiss="alert">&times;</button>
                </div>
            {% endif %}
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title"><i class="fa fa-pencil"></i> {{text_edit }}</h3>
                </div>
                <div class="panel-body">
                    <form action="{{ action }}" method="post" enctype="multipart/form-data" id="form-category"
                          class="form-horizontal">
                        <div class="form-group">
                            <label class="col-sm-2 control-label"
                                   for="input-status"> {{ entry_status }} </label>
                            <div class="col-sm-10">
                                <select name="wuunder_api" id="input-status" class="form-control">
                                    {% if wuunder_api %}
                                        <option value="1" selected="selected">{{ live_api }}</option>
                                        <option value="0">{{ staging_api }}</option>
                                    {% else %}
                                        <option value="1">{{ live_api }} </option>
                                        <option value="0" selected="selected">{{ staging_api }}</option>
                                    {% endif %}
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label"
                                   for="input-staging-key">{{ text_staging_api }}</label>
                            <div class="col-sm-10">
                                <input type="text" id="input-staging-key" name="wuunder_staging_key"
                                       class="form-control" value="{{ staging_key }}"/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label"
                                   for="input-live-key">{{ text_live_api }}</label>
                            <div class="col-sm-10">
                                <input type="text" id="input-live-key" name="wuunder_live_key" class="form-control"
                                       value="{{ live_key}}"/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label"
                                   for="wuunder_business">{{ text_business }}</label>
                            <div class="col-sm-10">
                                <input type="text" id="wuunder_business" name="wuunder_business" class="form-control"
                                       value="{{ business }}"/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label"
                                   for="wuunder_email_address">{{ text_email_address }}</label>
                            <div class="col-sm-10">
                                <input type="text" id="wuunder_email_address" name="wuunder_email_address"
                                       class="form-control" value="{{ email_address }}"/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label"
                                   for="wuunder_family_name">{{ text_family_name }}</label>
                            <div class="col-sm-10">
                                <input type="text" id="wuunder_family_name" name="wuunder_family_name"
                                       class="form-control" value="{{ family_name }}"/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label"
                                   for="wuunder_given_name">{{ text_given_name }}</label>
                            <div class="col-sm-10">
                                <input type="text" id="wuunder_given_name" name="wuunder_given_name"
                                       class="form-control" value="{{ given_name }}"/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label"
                                   for="wuunder_locality">{{ text_locality }}</label>
                            <div class="col-sm-10">
                                <input type="text" id="wuunder_locality" name="wuunder_locality" class="form-control"
                                       value="{{ locality }}"/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label"
                                   for="wuunder_phone_number">{{ text_phone_number }}</label>
                            <div class="col-sm-10">
                                <input type="text" id="wuunder_phone_number" name="wuunder_phone_number"
                                       class="form-control" value="{{ phone_number }}"/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label"
                                   for="wuunder_street_name">{{ text_street_name }}</label>
                            <div class="col-sm-10">
                                <input type="text" id="wuunder_street_name" name="wuunder_street_name"
                                       class="form-control" value="{{ street_name }}"/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label"
                                   for="wuunder_house_number">{{ text_house_number }}</label>
                            <div class="col-sm-10">
                                <input type="text" id="wuunder_house_number" name="wuunder_house_number"
                                       class="form-control" value="{{ house_number }}"/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label"
                                   for="wuunder_zip_code">{{ text_zip_code }}</label>
                            <div class="col-sm-10">
                                <input type="text" id="wuunder_zip_code" name="wuunder_zip_code" class="form-control"
                                       value="{{ zip_code }}"/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label"
                                   for="wuunder_country">{{ text_country }}</label>
                            <div class="col-sm-10">
                                <input type="text" id="wuunder_country" name="wuunder_country" class="form-control"
                                       value="{{ country }}"/>
                            </div>
                        </div>

                        <div class="form-group">
                          <label class="col-sm-2 control-label"
                                 for="wuunder_status">Status</label>
                          <div class="col-sm-10">
                            <select name="module_wuunder_status" id="input-status" class="form-control">
                              {% if module_wuunder_status %}
                                <option value="1" selected="selected">{{ text_enabled }}</option>
                                <option value="0">{{ text_disabled }}</option>
                              {% else %}
                                <option value="1">{{ text_enabled }}</option>
                                <option value="0" selected="selected">{{ text_disabled }}</option>
                              {% endif %}
                            </select>
                          </div>
                        </div>

                        <h2>{{ text_advanced_section }}</h2>
                        <div class="well well-lg">
                            <div class="form-group">
                                <label class="col-sm-2 control-label"
                                       for="wuunder_custom_housenumber">{{ text_custom_field_housenumber }}</label>
                                <div class="col-sm-10">
                                    <input type="text" id="wuunder_custom_housenumber" name="wuunder_custom_housenumber"
                                           class="form-control" value="{{ custom_housenumber }}"/>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
{{ footer }}
